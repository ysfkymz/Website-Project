@using stajprojesi.Models.EntitiyFramework

@model List<komisyon>

@{
    ViewBag.Title = "Yenikomisyon";
    Layout = "~/Views/Shared/_Layout4.cshtml";
}

<div class="container">
    <label>Oluşturmak İstediğiniz Komisyonun Adını Giriniz</label>

    <input type="text" name="firstname">


    <div style=" width:700px; height:20px;"> </div>
    <table class="table table-bordered" id="asdf">
        <thead>
            <tr>
                <th>No</th>
                <th>Ünvan</th>
                <th>Ad</th>
                <th>Soyad</th>

                <th>Ekle</th>


            </tr>
        </thead>
        <tbody>
            @foreach (var komisyon in Model)
            {
                <tr>
                    <td>@komisyon.id</td>
                    <td>@komisyon.unvan</td>
                    <td>@komisyon.ad</td>
                    <td>@komisyon.soyad</td>

                    <td><a class="btn btn-warning">Komisyona Ekle</a></td>


                </tr>
            }
        </tbody>
    </table>

    <div style="width:10px; height:120px">
</div>

</div>

