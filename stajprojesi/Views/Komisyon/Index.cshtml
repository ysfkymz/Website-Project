@using stajprojesi.Models.EntitiyFramework
@model List<komisyon>
@{

    Layout = "~/Views/Shared/_Layout4.cshtml";

<div class="container">
    @Html.ActionLink("Yeni Görevli Ekle", "Yeni", null, new { @class = "btn btn-success" })
    @Html.ActionLink("Komisyonlara Bak", "Komisyonlar", null, new { @class = "btn btn-success" })
    @Html.ActionLink("Komisyon Oluştur", "Yenikomisyon", null, new { @class = "btn btn-success" })

    <div style=" width:700px; height:20px;"> </div>
    <table class="table table-bordered" id="asdf">
        <thead>
            <tr>
                <th>No</th>
                <th>Ünvan</th>
                <th>Ad</th>
                <th>Soyad</th>
                <th>Güncelle</th>
                <th>Sil</th>


            </tr>
        </thead>
        <tbody>
            @foreach (var komisyon in Model)
            {
                <tr>
                    <td>@komisyon.id</td>
                    <td>@komisyon.unvan</td>
                    <td>@komisyon.ad</td>
                    <td>@komisyon.soyad</td>
                    <td><a class="btn btn-primary" href="/Komisyon/Guncelle/@komisyon.id">Güncelle</a></td>
                    <td><a class="btn btn-warning" href="/Komisyon/Sil/@komisyon.id">Sil</a></td>


                </tr>
            }
        </tbody>

    </table>
    <div style="width:10px; height:120px">
</div>



</div>
}









