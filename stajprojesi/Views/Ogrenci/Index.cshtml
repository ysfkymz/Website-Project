@using stajprojesi.Models.EntitiyFramework
@model List<staj>
@{

    Layout = "~/Views/Shared/_Layout7.cshtml";
}
<div class="container">


    @Html.ActionLink("Öğrenci Ekle", "Yeni", null, new { @class = "btn btn-success" })

    <div style=" width:700px; height:20px;"> </div>
    <table class="table table-bordered" id="asdf">
        <thead>
            <tr>
                <th>No</th>
                <th>Ad</th>
                <th>Soyad</th>
                <th>Öğretim</th>
                <th>Durum</th>
                <th>Güncelle</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var staj in Model)
            {
                <tr>
                    <td>@staj.ogr_no</td>
                    <td>@staj.ad</td>
                    <td>@staj.soyad</td>
                    <td>@staj.ogretim</td>
                    <td>@staj.durum</td>


                    <td><a class="btn btn-info">Güncelle</a></td>
                </tr>
            }
        </tbody>
    </table>

    <div style="width:10px; height:120px">
</div>

</div>